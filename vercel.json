{
  "version": 2,
  "name": "prompt-library",
  "builds": [
    {
      "src": "backend/server-supabase.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/server-supabase.js"
    },
    {
      "src": "/(.*)",
      "dest": "backend/server-supabase.js"
    }
  ]
}