{
  "name": "prompt-library",
  "version": "1.0.0",
  "description": "AI Prompt Library for Directors & Product Managers",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "npx prisma generate",
    "postinstall": "npx prisma generate",
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend:dev": "cd backend && npm run dev",
    "frontend:dev": "cd frontend && npm start"
  },
  "keywords": ["ai", "prompts", "director", "pm", "management"],
  "author": "Director/PM",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "@supabase/supabase-js": "^2.91.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.0",
    "prisma": "^5.15.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=20.19.0"
  }
}