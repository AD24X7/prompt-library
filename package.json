{
  "name": "prompt-library",
  "version": "1.0.0",
  "description": "AI Prompt Library for Directors & Product Managers",
  "main": "server.js",
  "scripts": {
    "start": "npx prisma migrate deploy && node server.js",
    "build": "npm install && cd backend && npm install && npx prisma generate",
    "postinstall": "npx prisma generate",
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend:dev": "cd backend && npm run dev",
    "frontend:dev": "cd frontend && npm start"
  },
  "keywords": ["ai", "prompts", "director", "pm", "management"],
  "author": "Director/PM",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^7.2.0",
    "bcryptjs": "^3.0.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-microsoft": "^2.1.0",
    "prisma": "^7.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=20.19.0"
  }
}